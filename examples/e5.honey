
fib = (n) =>
    if(eq(n 1)
        () => 1
        () =>
            if(eq(n 2)
                () => 1
                () =>
                    add(
                        fib(sub(n 1))
                        fib(sub(n 2))
                    )
            )
        )
        
result = fib(7)

out("The answer is" result)
